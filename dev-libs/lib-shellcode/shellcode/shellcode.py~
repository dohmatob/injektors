import struct

class AsmInstruction:
    def __init__(self,
                 opcodes,
                 mnemonics,
                 ):
        self._opcodes = opcodes;
        self._mnemonic = mnemonic
    
    def getOpcodes(self):
        return self._opcodes

    def getMnemonic(self):
        return self._mnemonic

    def display(self):
        print self._mnemonic


